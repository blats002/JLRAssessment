<?xml version='1.0' encoding='utf-8'?>
<widget id="co.uk.jaguarlandroverassessment" version="0.0.12" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>JLR Assessment</name>
    <description>
        A simple Assessment tool for JLR.
    </description>
    <author email="ramil.saavedra@gmail.com.com" href="http://phonegap.com">
        Ramil Saavedra FreeLancer
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="mailto:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="StatusBarOverlaysWebView" value="false" />
</widget>
