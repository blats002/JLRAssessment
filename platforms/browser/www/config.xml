<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jlr.assessment" version="0.0.8" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>JLR Assessment</name>
    <description>
        A simple Assessment tool for JLR.
            <icon src="icon.png" />
</description>
    <author email="ramil.saavedra@gmail.com.com" href="http://phonegap.com">
        Ramil Saavedra FreeLancer
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <icon src="images/icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
